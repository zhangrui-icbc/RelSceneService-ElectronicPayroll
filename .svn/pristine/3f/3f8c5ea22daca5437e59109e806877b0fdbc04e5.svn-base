package com.icbc.rel.hefei.util;

public class PageHandle {

    public static int pageOffer(int page_num,int page_size){
        int offer = 0;
        if(page_num <= 0 || page_size <= 0){
            return -1;
        }
        offer = (page_num - 1) * page_size;
        return offer;
    }

}
